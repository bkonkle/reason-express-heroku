{
  "name": "reason-graphql-docker",
  "version": "0.1.0",
  "description": "A demo deployment of ReasonML and Apollo GraphQL on Docker",
  "repository": {
    "type": "git",
    "url": "git@github.com:ecliptic/reason-graphql-docker.git"
  },
  "author": "Brandon Konkle <brandon@ecliptic.io> (http://ecliptic.io)",
  "license": "BSD-2-Clause",
  "keywords": [
    "reasonml"
  ],
  "scripts": {
    "build": "bsb -make-world",
    "build.clean": "bsb -clean-world",
    "build.watch": "yarn build -w",
    "dev": "cross-env NODE_ENV=development dotenv node ./src/Server.bs.js",
    "dev.debug": "cross-env DEBUG=paperclip-api:*,bs-knex:*,knex:* yarn dev",
    "dev.watch": "chokidar --initial \"src/**/*.js\" -c \"yarn dev\"",
    "lint": "eslint --ext .js,.jsx ./",
    "migrate": "dotenv knex migrate:latest",
    "postinstall": "rm -rf node_modules/bs-apollo-server-express/node_modules/bs-express",
    "precommit": "lint-staged",
    "prepublish": "yarn build",
    "prepush": "yarn test",
    "start": "cross-env NODE_ENV=production node ./src/Server.bs.js",
    "test": "cross-env NODE_ENV=test jest",
    "test.watch": "cross-env NODE_ENV=test jest --watch"
  },
  "dependencies": {
    "@glennsl/bs-json": "^1.1.2",
    "apollo-server-express": "^1.3.2",
    "body-parser": "^1.18.2",
    "bs-abstract": "^0.10.0",
    "bs-apollo-server-express": "^0.3.1",
    "bs-batteries": "^0.0.14",
    "bs-chalk": "^0.2.0",
    "bs-express": "^0.0.6",
    "bs-graphql": "^0.3.1",
    "bs-graphql-scalar": "^0.2.0",
    "bs-graphql-tools": "^0.3.1",
    "bs-knex": "^0.10.2",
    "bs-moment": "^0.2.1",
    "bs-node-debug": "^0.2.0",
    "bs-promise-router": "^0.2.0",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "graphql-tools": "^2.18.0",
    "morgan": "^1.9.0",
    "parse-database-url": "^0.3.0",
    "pg": "^7.4.1"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.3.3",
    "@playlyfe/gql": "^2.5.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "bs-platform": "2.1.0",
    "chokidar-cli": "^1.2.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.4",
    "jest-transform-graphql": "^2.1.0",
    "lint-staged": "^6.0.1",
    "prettier-eslint-cli": "^4.7.0"
  },
  "jest": {
    "testURL": "http://localhost",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom"
    ],
    "testMatch": [
      "**/?(*)(Spec|Test).js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "transform": {
      ".(js|jsx)": "babel-jest"
    },
    "testEnvironment": "node"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier-eslint --write",
      "git add"
    ],
    "src/**/*.re": [
      "refmt --in-place",
      "git add"
    ]
  }
}
